@model List<Duda>
@{
    int idP = 0;
    if (Model.Count != 0)
    {
        foreach (var d in Model)
        {
            <div>Duda: @d.Duda1</div>
            <div>Fecha: @d.FechaCreacion</div>
            idP = d.IdPropiedad;
        }
    }
    else
    {
        <h1>No hay datos</h1>
        foreach (var d in Model)
        {
            idP = d.IdPropiedad;
        }
    }

    PROYECTOISW.Models.ViewModel.ContactarViewModel.ContactarViewModel duda = new PROYECTOISW.Models.ViewModel.ContactarViewModel.ContactarViewModel();
    duda.IdPropiedad = idP;
    @await Html.PartialAsync("~/Views/Busqueda/CrearDuda.cshtml", duda)
}