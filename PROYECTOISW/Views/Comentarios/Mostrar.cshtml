@model List<Reseña>

<h1>Comentarios</h1>

@foreach (var r in Model)
{
    <h1>Nombre del alumno: @r.IdUsuarioNavigation.NombreCompleto</h1>
    <h1>
        @for (int i = 0; i < r.Calificacion; i++)
        {
            <span>&#9733;</span> <!-- Esto es una estrella -->
        }
    </h1>
    <h1>@r.Comentario</h1>
}
